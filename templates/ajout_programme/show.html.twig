{% extends 'base.html.twig' %}

{% block title %}Programme{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href={{asset('css/crud/index.css')}}>
{% endblock %}
{% block body %}
<div class="box">
    <h1>Programme</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ programme.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ programme.name }}</td>
            </tr>
        </tbody>
    </table>

    <a class="button" href="{{ path('app_ajout_programme_index') }}">Retour a la liste</a>

    <a class="button" href="{{ path('app_ajout_programme_edit', {'id': programme.id}) }}">Modifier</a>

    {{ include('ajout_programme/_delete_form.html.twig') }}
    </div>
{% endblock %}
